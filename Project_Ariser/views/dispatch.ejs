															<label for="hsnnumber">HSN:</label>
															<input type="number" class="form-control"  id="hsnnumber" name="hsnnumber" placeholder="Enter HSN"></input>
															<p id="operationvalidate" class = "text-warning"></p>
														</div>
					
													</div>
													
														
													
													<div class="row form-group">
														
														<div class="col-sm-4">
															<label for="Description">Description:</label>
															<input type="Text" class="form-control" placeholder="Enter Description" id="Description" name="Description"></input>
															<p id="descriptionvalidate" class = "text-warning"></p>
														</div>
					
														<div class="col-sm-4">
																<label for="operation">Operation:</label>
																<input type="number" class="form-control"  id="operation" name="operation" placeholder="Enter Operation"></input>
																<p id="operationvalidate" class = "text-warning"></p>
														</div>
						
														<div class="col-sm-4">
															<label for="pricepiece">Price per piece</label>
															<input type="Number" class="form-control" id="pricepiece" name="pricepiece" placeholder="Enter Price per Piece">
														</div>
					
													</div>
					
												</div>
									</div>

								</div>

							<div class="row form-group">
								<div class="col-sm-4">
									<label for="customer">Customer</label>
									<div>
										<div class="autocomplete customer">
											<input id="customer"  class="form-control" type="text" name="customer" placeholder="Customer" required="true" readonly>
										</div>
									</div>
								</div>
								<div id = "NewCustomer" style="Display:none" >
									<div class="container">

											<div class="row form-group">

											  <div class="col-sm-6">
												<label for="gstin">GSTIN</label>
												<input type="text" class="form-control" name="gstin" id="gstin" placeholder="Enter GSTIN" >
											</div>
											</div>
			
											<div class="row form-group">
												<div class="col-sm-4">
													  <label for="AddressOne">Address Line One</label>
													<input type="text" class="form-control" name="AddressOne" id="AddressOne" placeholder="Enter Address">
											  </div>
											  <div class="col-sm-4">
												<label for="City">City</label>
												<input type="text" class="form-control" name="City" id="City" placeholder="Enter City">
											</div>
											</div>
											
											<div class="row form-group">
			
												<div class="col-sm-4">
													<label for="AddressTwo">Address Line Two</label>
