											<div class="autocomplete" style="padding:0px 0px 0px 40px;background-color:white; width:350px;">
												<input name="customername" id="customer" type="text" placeholder="Company Name" readonly></input>
											</div><br>
											<input name="customeraddress1" type="text" placeholder="Address 1" id="customeraddress1" style="padding:0px 0px 0px 50px;background-color:white;" readonly></input><br>
											<input name="customeraddress2" id="customeraddress2" type="text" placeholder="Address 2" style="padding:0px 0px 0px 50px;background-color:white;" readonly><br>
											<div class="row"><div  class="col-md-4"><input name="city" id="city" type="text" placeholder="City" style="padding:0px 0px 0px 50px;background-color:white;" readonly></input></div><div class="col-md-4"><input name="pincode" id="pincode" type="text" placeholder="Pincode" style="padding:0px 0px 0px 50px;background-color:white;" readonly></input></div></div>
											<input name="gstin" id="gstin" type="text" placeholder="GSTIN" style="padding:0px 0px 0px 50px;background-color:white;" readonly></input></td>
