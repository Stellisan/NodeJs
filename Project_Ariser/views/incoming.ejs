									</tr>
                                    
                                    <tr>
                                        <td colspan="2" class="text-center" style="padding:20px 10px 20px 10px;vertical-align:middle;">
                                            <input id="dcno" class="text-center" name="dcno" type="text" placeholder="D.C. No" required/>
                                        </td>
                                        <td colspan="2" class="text-center" style="padding:20px 10px 20px 10px;vertical-align:middle;">
                                            <div class="input-group date" data-provide="datepicker">
                                                <input name="DCDate" id="DCDate" type="text" placeholder='D.C. Date' class="form-control" required="true">
                                                <div class="input-group-addon">
                                                    <span class="glyphicon glyphicon-th"></span>
                                                </div>
                                            </div>
                                        </td>
                                    </tr>

									<tr>
										<th>S.No</th>
										<th>Component Code</th>
                                        <th>Description</th>
                                        <th>Operation</th>
										<th>HSN</th>
										<th>QTY</th>
										<th>Rate/Unit</th>
									</tr>
									<tr id = "comp0" name="comp0">
										
										<td>
											1
										</td>

										<td>
												<div class="autocomplete" style="width:150px;">
													<input id="drawingcode"  class="drawingcode" type="text" name="drawingcode[]" placeholder="Code" onload="autocomplete(this,)" onkeydown="Option_Checker(this)" required>
												</div>
										</td>

										<td width=250>
												<input type="text" name='desc[]' placeholder="Select Product" class="form-control desc" step="0" min="0" style="border:0px; outline:0px;" readonly/>
										</td>

										<td>
                                                <input type="text" name='operation[]' placeholder='0' class="form-control operation" style="border:0px; outline:0px;" readonly/>
                                        </td>

										<td>
												<input type="number" name='hsn[]' placeholder='0' class="form-control hsn" step="0" min="0" style="border:0px; outline:0px;" readonly/>
										</td>

										<td>
											<input type="number" name='qty[]' placeholder='0' class="form-control qty" step="0" min="0" style="border:0px; outline:0px;" required/>
										</td>

										<td>
											<input type="number" name='price[]' placeholder='0.00' class="form-control price" min="0" style="border:0px; outline:0px;" required/>
										</td>

									</tr>
								
									<tr id='comp1' name="comp1 "></tr>
								</tbody>
